<h2>list of components</h2>

<select>
  {{#each model as |comps|}}
      <option data-voltage="{{comps.voltage}}" data-power="{{comps.power}}">{{comps.psuname}}</option>
  {{/each}}
</select>

<select>
  {{#each model as |comps|}}
