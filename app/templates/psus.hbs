  {{/each}}
</select>

<select>
  {{#each model as |comps|}}
      <option>{{comps.jmpname}}</option>
  {{/each}}
</select>

<select>
  {{#each model as |comps|}}
      <option>{{comps.wpxname}}</option>
  {{/each}}
</select>
